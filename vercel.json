{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "headers": [
    {
      "source": "/api-docs/(.*\\.css)",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "/api-docs/(.*\\.js)",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ],
  "outputDirectory": "build",
  "env": {
    "NODE_ENV": "production"
  }
}
